<!doctype html>

<html lang="en" ng-app="mlm">

<head>

    <!-- Basic -->
    <title>RMart | Home</title>

    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@

    <!-- Define Charset -->
    <meta charset="utf-8">

    <!-- Responsive Metatag -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Page Description and Author -->
    <meta name="description" content="Margo - Responsive HTML5 Template">
    <meta name="author" content="iThemesLab">



    <!-- Bootstrap CSS  -->
    <link rel="stylesheet" href="~/Content/Bootstrap/asset/css/bootstrap.min.css" type="text/css" media="screen">
    @*<link href="@Url.Content("~/Content/Bootstrap/asset/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />*@
    <!-- Font Awesome CSS -->
    <link rel="stylesheet" href="~/Content/Bootstrap/css/font-awesome.min.css" type="text/css" media="screen">
    @*<link href="@Url.Content("~/Content/Bootstrap/asset/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />*@
    <!-- Margo CSS Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/style.css" media="screen">
    @*<link href ="@Url.Content("~/Content/Bootstrap/css/style.css")" rel="stylesheet" type="text/css" />*@

    <!-- Responsive CSS Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/responsive.css" media="screen">

    <!-- Css3 Transitions Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/animate.css" media="screen">

    <!-- Color CSS Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/red.css" title="red" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/jade.css" title="jade" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/green.css" title="green" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/blue.css" title="blue" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/beige.css" title="beige" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/cyan.css" title="cyan" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/orange.css" title="orange" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/peach.css" title="peach" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/pink.css" title="pink" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/purple.css" title="purple" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/sky-blue.css" title="sky-blue" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Bootstrap/css/colors/yellow.css" title="yellow" media="screen" />


    <!-- Margo JS  -->
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery-2.1.1.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.migrate.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/modernizrr.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/asset/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.fitvids.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/nivo-lightbox.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.isotope.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.appear.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/count-to.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.textillate.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.lettering.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.easypiechart.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.nicescroll.min.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/jquery.parallax.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/mediaelement-and-player.js"></script>
    <script type="text/javascript" src="~/Content/Bootstrap/js/script.js"></script>

    <!--[if IE 8]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <!--[if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->





    <base href="@System.Configuration.ConfigurationManager.AppSettings["BaseUrlPath"].ToString()" />

</head>

<body>

    <!-- Full Body Container -->
    <div id="container" >


        <!-- Start Header Section -->
        <div class="hidden-header" style="height: 112px;"></div>
        <header class="clearfix">

            <!-- Start Top Bar -->
            <div class="top-bar" style="display: block;">
                <div class="container">
                    <div class="row">
                        <div class="col-md-7">
                            <!-- Start Contact Info -->
                            <ul class="contact-details">
                                <li>
                                    <a href="#"><i class="fa fa-map-marker"></i>204, 2nd Floor, Puneet Vrindavan Tower, Sanjay Place, Agra-2</a>
                                </li>
                                <li>
                                    <a href="#"><i class="fa fa-envelope-o"></i> info@recentmart.in</a>
                                </li>
                                <li>
                                    <a href="#"><i class="fa fa-phone"></i> 9897496428</a>
                                </li>
                            </ul>
                            <!-- End Contact Info -->
                        </div><!-- .col-md-6 -->
                        <div class="col-md-5">
                            <!-- Start Social Links -->
                            <ul class="social-list">
                                <li>
                                    <a class="facebook itl-tooltip" data-placement="bottom" title="Facebook" href="#"><i class="fa fa-facebook"></i></a>
                                </li>
                                <li>
                                    <a class="twitter itl-tooltip" data-placement="bottom" title="Twitter" href="#"><i class="fa fa-twitter"></i></a>
                                </li>
                            </ul>
                            <!-- End Social Links -->
                        </div><!-- .col-md-6 -->
                    </div><!-- .row -->
                </div><!-- .container -->
            </div><!-- .top-bar -->
            <!-- End Top Bar -->
            <!-- Start  Logo & Naviagtion  -->
            <div class="navbar navbar-default navbar-top">
                <div class="container">
                    <div class="navbar-header">
                        <!-- Stat Toggle Nav Link For Mobiles -->
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <i class="fa fa-bars"></i>
                        </button>
                        <!-- End Toggle Nav Link For Mobiles -->
                    </div>
                    <div class="navbar-collapse collapse">
                        <!-- Stat Search -->
                        <div class="search-side">
                            <a href="#" class="show-search"><i class="fa fa-search"></i></a>
                            <div class="search-form">
                                <form autocomplete="off" role="search" method="get" class="searchform" action="#">
                                    <input type="text" value="" name="s" id="s" placeholder="Search the site...">
                                </form>
                            </div>
                        </div>
                        <!-- End Search -->
                        <!-- Start Navigation List -->
                        @Html.Partial("_AdminNavigationPartial")
                        <!-- End Navigation List -->
                    </div>
                </div>
            </div>
            <!-- End Header Logo & Naviagtion -->

        </header>
        <!-- End Header Section -->
        <div ng-view></div>
        @RenderBody()

        <!-- Start Footer Section -->
        @Html.Partial("_FooterPartial")
        <!-- End Footer Section -->

    </div>
    <p>
        <!-- End Full Body Container -->
    </p>
    <p>

        <!-- Go To Top Link -->
        <a href="#" class="back-to-top"><i class="fa fa-angle-up"></i></a>

    </p> 
    <div id="loader">
        <div class="spinner">
            <div class="dot1"></div>
            <div class="dot2"></div>
        </div>
    </div>
    <!-- angular js scripts-->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/MLMJs")
    @Scripts.Render("~/bundles/bootstrap")
    @Styles.Render("~/bundles/mlmTheme")
    <script src="http://d3js.org/d3.v3.min.js"></script>
    @RenderSection("scripts", required: false)

</body>

</html>